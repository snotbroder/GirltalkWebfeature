---
const { bgcolor, src, alt } = Astro.props;
import "../styles/generel.css";
---

<img class={bgcolor} src={src} alt={alt} width="800" height="622" />

<style>
  img {
    border-radius: 5px;
  }

  .primarybg {
    box-shadow: 18px 18px 0px 2px var(--primary);
  }

  .brandcolorbg {
    box-shadow: 18px 18px 0px 2px var(--primary);
  }
</style>
